{
  "name": "dioxus-app",
  "version": "1.0.0",
  "dependencies": {
    "@tailwindcss/cli": "^4.1.7",
    "tailwindcss": "^4.1.7"
  },
  "scripts": {
    "css:build": "npx tailwindcss -i ./tailwind.css -o assets/tailwind.css",
    "css:watch": "npx tailwindcss -i ./tailwind.css -o assets/tailwind.css --watch",
    "dev": "npm run css:watch & ./dx serve"
  }
}
